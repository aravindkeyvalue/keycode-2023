  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf ./dist && ./node_modules/.bin/tsc",
    "dev": "NODE_ENV=development tsc-watch --onSuccess \"node ./dist/server.js\"",
    "template-migration": "npx tsc && ts-node ./node_modules/typeorm/cli --f ./dist/config/rdbms.js migration:create -n",
    "add-migration": "npm run build && NODE_ENV=development && node ./node_modules/typeorm/cli --f ./dist/config/rdbms.js migration:generate -n",
    "migrate": "npm run build && NODE_ENV=development node ./node_modules/typeorm/cli --f ./dist/config/rdbms.js migration:run",
    "rollback": "npm run build && node ./node_modules/typeorm/cli --f ./dist/config/rdbms.js migration:revert"
  },
